# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'CheetahChallenge' do
  # Comment the next line if you don't want to use dynamic frameworks
  inhibit_all_warnings!
  use_frameworks!
  pod 'RxSwift'
  pod 'RxCocoa'
  
  # Pods for CheetahChallenge
  pod 'SnapKit'
  pod 'SwiftLint'
  pod 'SwiftyAttributes'

  target 'CheetahChallengeTests' do
    #inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
    pod 'Fakery'
  end

end
